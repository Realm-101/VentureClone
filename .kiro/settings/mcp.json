{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "fetch"
      ]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--executablePath",
        "C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "list_pages",
        "new_page",
        "performance_start_trace"
      ]
    }
  }
}